  "contributors": ["kobakazu0429 <kobakazu10@gmail.com>"],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "types": "./esm/index.d.ts",
  "files": [
    "lib",
    "esm",
    "dist"
  ],
  "scripts": {
    "dev": "vite --config=vite.config.dev.ts",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "vite build"
  },
  "dependencies": {
    "lit": "^2.0.2",
    "nunjucks": "^3.2.3",
    "swiper": "^8.0.1",
    "three": "^0.105.2",
    "viewerjs": "^1.10.3"
  },
  "devDependencies": {
    "@types/nunjucks": "^3.2.1",
    "amd-to-es6": "^0.2.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-dts": "^1.0.4"
  }
}
